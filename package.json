{
  "name": "swarmfm-player",
  "version": "1.0.0",
  "description": "Swarm FM Player",
  "main": "main.js",
  "scripts": {
  "start": "electron .",
  "build": "electron-builder ",
  "release": "electron-builder build --publish always"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LukasDerBaum42/swarmfm-player.git"
  },
  "author": "LukasDerBaum",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^29.4.6",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {
    "electron-log": "^5.1.0",
    "electron-updater": "^6.1.7",
    "electron-window-state": "^5.0.3"
  },
  "build": {
    "appId": "fm.swarm.player",
    "productName": "Swarm FM Player",
    "publish": {
      "provider": "github",
      "owner": "LukasDerBaum42",
      "repo": "swarmfm-player"
    },
    "win": {
      "target": "nsis",
      "icon": "icon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "icon.png",
      "category": "public.app-category.music"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icon.png",
      "category": "Audio"
    }
  }
}
